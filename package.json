{
  "name": "viikon-kouluruoka-sdk",
  "version": "1.0.0",
  "description": "Viikon kouluruoka SDK for JavaScript",
  "main": "src/index.ts",
  "repository": "git@github.com:theel0ja/viikon-kouluruoka-sdk.git",
  "author": "Elias Ojala <elias@eliasojala.me>",
  "private": true,
  "scripts": {
    "start:dev": "webpack-dev-server --hot --mode=development",
    "build": "webpack --mode=production && yarn tslint",
    "deploy": "wsl ./deploySdk.sh",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "docs:dev": "docz dev --src=./docs",
    "docs:build": "docz build --src=./docs",
    "docs:deploy": "gh-pages -d .docz/dist"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "docz": "^0.2.6",
    "gh-pages": "^1.2.0",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "core-js": "^2.5.7"
  }
}
